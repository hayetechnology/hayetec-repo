package assignment4;

public class Truck extends Vehicle
{
  public String hands = "dont know";

  public Truck(String name, int cylinders, Person owner, double load,  int capacity)
  {
    super(name, cylinders, owner);
    this.load = load;
    this.capicity = capacity;
  }

  public void setWiderciever(String hands)
  {
    this.hands = hands;
  }
  public String getHands()
  {
    return hands;
  }
}
